GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = ${PODS_ROOT}/../ffmpeg/include ${PODS_ROOT}/pili-ffmpeg/pili-ffmpeg/include ${PODS_ROOT}/pili-ffmpeg/pili-ffmpeg/include $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/HappyDNS" "${PODS_ROOT}/Headers/Public/PLPlayerKit" "${PODS_ROOT}/Headers/Public/pili-ffmpeg" "${PODS_ROOT}/Headers/Public/pili-librtmp"
LIBRARY_SEARCH_PATHS = $(inherited) ${PODS_ROOT}/../ffmpeg/lib "$PODS_CONFIGURATION_BUILD_DIR/HappyDNS" "$PODS_CONFIGURATION_BUILD_DIR/PLPlayerKit" "$PODS_CONFIGURATION_BUILD_DIR/pili-librtmp" "${PODS_ROOT}/PLPlayerKit/Pod/Library/lib" "${PODS_ROOT}/pili-ffmpeg/pili-ffmpeg/lib"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/HappyDNS" -isystem "${PODS_ROOT}/Headers/Public/PLPlayerKit" -isystem "${PODS_ROOT}/Headers/Public/pili-ffmpeg" -isystem "${PODS_ROOT}/Headers/Public/pili-librtmp"
